{
  "name": "11ty-starter",
  "version": "1.0.0",
  "scripts": {
    "build:css": "postcss src/css/main.css --dir public --env production",
    "build:eleventy": "ELEVENTY_ENV=production eleventy --config=src/site/.eleventy.js",
    "build:js": "rollup -c",
    "watch:css": "postcss src/css/main.css --dir public --watch --verbose",
    "watch:eleventy": "ELEVENTY_ENV=development eleventy --serve --config=src/site/.eleventy.js",
    "watch:js": "rollup -c -w",
    "build": "run-s clean build:css build:js build:eleventy",
    "clean": "rimraf public",
    "start": "npm-run-all clean --parallel watch:*"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.10.0",
    "@fullhuman/postcss-purgecss": "^1.3.0",
    "@rollup/plugin-buble": "^0.21.1",
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@rollup/plugin-replace": "^2.3.1",
    "@tailwindcss/custom-forms": "^0.2.1",
    "autoprefixer": "^9.7.4",
    "cssnano": "^4.1.10",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.0",
    "postcss-hash": "^2.0.0",
    "postcss-preset-env": "^6.7.0",
    "rimraf": "^2.6.3",
    "rollup": "^1.31.0",
    "rollup-plugin-output-manifest": "^1.0.2",
    "rollup-plugin-terser": "^5.2.0",
    "tailwindcss": "^1.2.0"
  },
  "dependencies": {
    "fuse.js": "^3.4.6",
    "yall-js": "^3.1.7"
  },
  "browserslist": [
    "defaults"
  ]
}
